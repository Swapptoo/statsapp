{
    "name": "webspark/statsapp",
    "license": "GPL-3.0",
    "type": "project",
    "description": "Application for collecting and processing statistics from remote applications",
    "autoload": {
        "psr-4": {
            "Mautic\\StatsBundle\\": "src/"
        },
        "files": [
            "app/AppCache.php",
            "app/AppKernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mautic\\StatsBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/class-loader": "2.8.*",
        "symfony/config": "2.8.*",
        "symfony/console": "2.8.*",
        "symfony/dependency-injection": "2.8.*",
        "symfony/event-dispatcher": "2.8.*",
        "symfony/finder": "2.8.*",
        "symfony/framework-bundle": "2.8.*",
        "symfony/http-foundation": "2.8.*",
        "symfony/http-kernel": "2.8.*",
        "symfony/monolog-bundle": "~2.4",
        "symfony/options-resolver": "2.8.*",
        "symfony/routing": "2.8.*",
        "symfony/security": "2.8.*",
        "symfony/templating": "2.8.*",
        "symfony/translation": "2.8.*",
        "symfony/twig-bridge": "2.8.*",
        "symfony/twig-bundle": "2.8.*",
        "symfony/validator": "2.8.*",
        "symfony/yaml": "2.8.*",
        "babdev/mautic-bundle": "dev-master",
        "doctrine/orm": "~2.5 >=2.5.2",
        "doctrine/doctrine-bundle": "~1.6",
        "ekino/newrelic-bundle": "~1.3 >=1.3.2",
        "friendsofsymfony/rest-bundle": "~1.7",
        "jms/serializer-bundle": "~1.1",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0",
        "twig/extensions": "~1.0",
        "twig/twig": "~1.23"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "liip/functional-test-bundle": "~1.3",
        "sensio/generator-bundle": "~3.0",
        "symfony/css-selector": "2.8.*",
        "symfony/debug": "2.8.*",
        "symfony/dom-crawler": "2.8.*",
        "symfony/phpunit-bridge": "2.8.*",
        "symfony/web-profiler-bundle": "2.8.*",
        "phpunit/phpunit": "~4.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-var-dir": "var",
        "symfony-web-dir": "www",
        "symfony-assets-install": "relative"
    }
}
